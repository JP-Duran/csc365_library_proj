<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<!-- IMPORTANT
If the program isn't running try to make sure that you add the " fx:controller="project.finalproject.projectController " back into the < ~~~ > down below. Add
in as the last argument
-->

<AnchorPane prefHeight="182.0" prefWidth="687.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="project.finalproject.projectController">
   <children>
      <Label fx:id="welcomeText" layoutX="9.0" layoutY="27.0" text="Welcome to the SLO County Library">
         <font>
            <Font name="Segoe Print" size="36.0" />
         </font>
      </Label>
      <Button fx:id="userButton" layoutX="182.0" layoutY="106.0" onAction="#onUser" text="User Login" />
      <Button fx:id="employeeButton" layoutX="361.0" layoutY="106.0" mnemonicParsing="false" onAction="#onEmployee" text="Employee Login" />
   </children>
</AnchorPane>
